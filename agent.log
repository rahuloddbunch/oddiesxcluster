n}\n\n.price-sec-main .price {\n  font-size: 2rem;\n  font-weight: 700;\n  color: #007431;  /* Oddies green */\n}\n\n.price-sec-main .price__regular {\n  display: flex;\n  gap: 0.5rem;\n  align-items: baseline;\n}\n\n.price-sec-main .price__sale {\n  color: #d32f2f;  /* Sale red */\n}\n\n/* View Box button enhancement */\n.view-box {\n  width: 100%;\n  background-color: #1a1a1a;\n  color: #ffffff;\n  border: none;\n  border-radius: 8px;\n  padding: 1.2rem 2rem;\n  font-size: 1.5rem;\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 0.05em;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  margin-top: 1.2rem;\n  text-align: center;\n  display: block;\n}\n\n.view-box:hover {\n  background-color: #007431;  /* Oddies green on hover */\n  transform: translateY(-1px);\n  box-shadow: 0 4px 12px rgba(0, 116, 49, 0.3);\n}\n\n/* Bestseller badge styling */\n.badge--bestseller {\n  background-color: #FFD700;  /* Gold background */\n  color: #1a1a1a;\n  font-weight: 700;\n  font-size: 1.1rem;\n  padding: 0.6rem 1.4rem;\n  border-radius: var(--badge-corner-radius);\n  text-transform: uppercase;\n  letter-spacing: 0.08em;\n  box-shadow: 0 2px 8px rgba(255, 215, 0, 0.4);\n}\n\n.badge--bestseller::before {\n  content: "🔥 ";\n  margin-right: 0.3rem;\n}\n\n/* Ensure badges don\'t overlap with enhanced cards */\n.card__badge {\n  z-index: 2;\n}\n\n/* Responsive adjustments */\n@media screen and (max-width: 749px) {\n  .product-card-wrapper .card__heading {\n    font-size: 1.6rem;\n  }\n\n  .price-sec-main .price {\n    font-size: 1.8rem;\n  }\n\n  .view-box {\n    padding: 1rem 1.5rem;\n    font-size: 1.4rem;\n  }\n}\n\n/* Remove quick-add buttons (ensuring they don\'t show) */\n.quick-add {\n  display: none !important;\n}\n\n.quick-add__submit {\n  display: none !important;\n}'})], model='claude-sonnet-4-5-20250929', parent_tool_use_id=None)
